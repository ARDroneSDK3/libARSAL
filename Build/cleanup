#! /bin/sh

if [ -e Makefile ] ; then
	make clean
fi

# Generated files.
rm -f \
    INSTALL \
    Makefile \
    Makefile.in \
    aclocal.m4 \
    config.h \
    config.h.in \
    config.log \
    config.status \
    configure \
    stamp-h1

# Generated directories.
rm -rf \
    doxygen-doc \
    autom4te.cache \
    Debug \
    Release \
    config \
    `find . -name '.deps'`	\
    `find . -name '*.*~'`
