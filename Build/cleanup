#! /bin/sh

if [ -e Makefile ] ; then
	make clean
fi

# Generated files.
rm -f \
    INSTALL \
    Makefile \
    Makefile.in \
    aclocal.m4 \
    config.h \
    config.h.in \
    config.h.in~ \
    config.log \
    config.status \
    configure \
    libtool \
    stamp-h1

# Generated directories.
rm -rf \
    doxygen-doc \
    autom4te.cache \
    *DSYM* \
    config \
    m4 \
    libsal_dbg.la \
    libsal.la \ 
    `find .. -name '.deps'`	\
    `find .. -name '.dirstamp'`

